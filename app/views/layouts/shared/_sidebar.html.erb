      <!-- Sidebar user panel -->
      <div class="user-panel">
        <div class="pull-left image">
          <%#=link_to(image_tag(current_user.avatar.url(:small), class: "img-circle", width: 40, height: 40), settings_employee_path(current_user)) %>
        </div>
        <div class="pull-left info">
          <p><%#=link_to current_user.first_name, settings_employee_path(current_user) %></p>
          <a href="#"><i class="fa fa-circle text-success"></i> <small>Online</small></a>
        </div>
      </div>
      <!-- search form -->
      <%#=form_tag result_index_path, method: :get, class: "sidebar-form" do %>
      <div class="input-group">
